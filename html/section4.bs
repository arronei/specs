    <code class="no-backref">formnovalidate</code>,
    <code class="no-backref">formtarget</code>,
    <code class="no-backref">height</code>,
    <code class="no-backref">inputmode</code>,
    <code class="no-backref">list</code>,
    <code class="no-backref">max</code>,
    <code class="no-backref">maxlength</code>,
    <code class="no-backref">min</code>,
    <code class="no-backref">minlength</code>,
    <code class="no-backref">multiple</code>,
    <code class="no-backref">pattern</code>,
    <code class="no-backref">placeholder</code>,
    <code class="no-backref">readonly</code>,
    <code class="no-backref">size</code>,
